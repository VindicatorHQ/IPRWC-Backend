settings:
  redirectUrl: http://localhost:4200/login
admin:
  name:
  lastName:
  email:
  password:
jwt:
  secret:
spring:
  mail:
    host: smtp.gmail.com
    port: 587
    username:
    password:
    properties:
      mail:
        smtp:
          auth: true
          starttls:
            enable: true
  datasource:
    url: jdbc:mysql://localhost:3306/iprwc-webshop
    username: root
    password:
    driverClassName: com.mysql.cj.jdbc.Driver
  jpa:
    database-platform: org.hibernate.dialect.MySQLDialect
    show-sql: true
    hibernate:
      ddl-auto: create-drop
server:
  error:
    include-message: never
    include-exception: true
  servlet:
    context-path: /v1/api
  port: 8081
